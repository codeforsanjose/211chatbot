{
  "$schema": "../../assets/modules/channel-web/config.schema.json",
  "uploadsUseS3": false,
  "uploadsS3Bucket": "bucket-name",
  "uploadsS3Region": "eu-west-1",
  "uploadsS3AWSAccessKey": "your-aws-key-name",
  "uploadsS3AWSAccessSecret": "secret-key",
  "startNewConvoOnTimeout": true,
  "recentConversationLifetime": "6 hours",
  "maxMessageLength": 360,
  "showBotInfoPage": true,
  "fileUploadPath": "./uploads",
  "infoPage": {
    "enabled": true
  },
  "maxMessagesHistory": 20,
  "security": {
    "escapeHTML": false
  },
  "chatUserAuthDuration": "24h",
  "lazySocket": false
}
